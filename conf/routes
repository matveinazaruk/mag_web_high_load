# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET 	/events						controllers.EventsController.events()
POST 	/events 					controllers.EventsController.createEvent()

GET		/events/:id/tickets 		controllers.EventsController.getTickets(id: String)
POST 	/events/:id/ticlets			controllers.EventsController.createTickets(id: String)